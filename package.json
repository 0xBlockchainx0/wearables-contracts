    "@nomiclabs/buidler-truffle5": "^1.0.1",
    "@nomiclabs/buidler-web3": "^1.0.1",
    "@openzeppelin/contracts": "^2.3.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
